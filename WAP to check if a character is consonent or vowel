#include<stdio.h>
#include<ctype.h>
char main()
{
char ch;
printf("Enter the character to check if it is vowel or consonent\n");
scanf("%c",&ch);
ch=(char)tolower(ch);
if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
printf("character is vowel\n");
else
printf("character is consonent\n");
return 0;
}
