#include<stdio.h>
#include<math.h>
#include<ctype.h>
int main()
{
    double a, b;
    char ch;
    printf("Enter two operands\n");
    scanf("%f%f",&a,&b);
    printf("Enter the operator");
    scanf(" %c",&ch);
    switch(ch)
    {
        case '+':
        printf("a+b=%f\n",(a+b));
        break;
        case '-':
        printf("a-b=%f\n",(a-b));
        break;
        case '*':
        printf("a*b=%f\n",(a*b));
        break;
        case '/':
        printf("a/b=%f\n",(a/b));
        break;
        
    }
    return 0;
}
