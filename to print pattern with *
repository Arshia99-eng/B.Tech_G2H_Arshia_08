#include <stdio.h>

int main() {
    int i,j;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=(i+4);j++)
        {
            if((i+j)==6||(i+j)==8||(i+j)==10||(i+j)==12||(i+j)==14)
            printf("*");
            else
            printf(" ");
        }
        printf("\n");
    }

    return 0;
}

  
